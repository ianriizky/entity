{
    "name": "ianrizky/entity",
    "type": "library",
    "description": "Handle data transfer object pattern in magic way",
    "authors": [
        {
            "name": "ianriizky",
            "email": "ian.rizkypratama@gmail.com"
        }
    ],
    "keywords": [
        "entity",
        "datatransferobject"
    ],
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0",
        "symfony/var-dumper": "^5.1"
    },
    "require-dev": {
        "filp/whoops": "^2.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ianrizky\\Entity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true
}
